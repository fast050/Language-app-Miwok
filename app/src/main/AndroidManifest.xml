<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fast050.miwokapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MiwokApp">
        <activity android:name=".Phrases"
            android:label="@string/PhrasesFragment_name"
            android:parentActivityName=".MainActivity"
            >

        </activity>
        <activity android:name=".Colors"
            android:label="@string/ColorsFragment_name"
            android:parentActivityName=".MainActivity"
           >
        </activity>
        <activity android:name=".FamilyMembers"
            android:label="@string/FamilyMembersFragment_name"
            android:parentActivityName=".MainActivity"
            >
        </activity>

        <activity android:name=".Numbers"
                  android:label="@string/NumbersFragment_name"
            android:parentActivityName=".MainActivity"
           >


        </activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>